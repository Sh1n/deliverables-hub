<%@ taglib prefix="s" uri="/struts-tags" %>
<!-- Top Breadcrumb Start -->
    <div id="breadcrumb">
    	<ul>	
            <li><img src="<s:url value="/assets/images/icons/icon_breadcrumb.png"/>" alt="Location" /></li>
            <li><strong>Location:</strong></li>
            <li><a href="#" title=""><s:text name="%{'menu_head'}.%{#session.namespace.replace(\"/\", \"\")}" /></a></li>
            <li>/</li>
            <li class="current"><s:text name="%{'menu_item'}.%{#session.action}" /></li>
        </ul>
    </div>
    <!-- Top Breadcrumb End -->